    "@types/lodash": "^4.14.186",
    "bcryptjs": "^2.4.3",
    "husky": "^8.0.1",
    "lodash": "^4.17.21",
    "next": "12.3.1",
    "next-auth": "^4.12.3",
    "prettier": "^2.7.1",
    "react": "18.1.0",
    "react-animate-height": "^3.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.37.0",
    "react-toastify": "^9.1.1",
    "sweetalert2": "^11.6.16",
    "sweetalert2-react-content": "^5.0.7"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} db/seed.ts"
